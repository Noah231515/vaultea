{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": 2,
    "no-debugger": 2,
    "no-extra-semi": 2,
    "no-inferrable-types": 0,
    "quotes": [2, "double", "avoid-escape"]
  }
}
